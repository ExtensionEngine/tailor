{
  "name": "@tailor-cms/core-components",
  "description": "Reusable Tailor UI components",
  "author": "ExtensionEngine <info@extensionengine.com>",
  "license": "MIT",
  "version": "1.0.0-beta.15",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/core-components.cjs",
  "umd:main": "./dist/core-components.umd.cjs",
  "module": "./dist/core-components.js",
  "exports": {
    ".": {
      "import": "./dist/core-components.js",
      "require": "./dist/core-components.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "homepage": "https://github.com/ExtensionEngine/tailor#readme",
  "bugs": {
    "url": "https://github.com/ExtensionEngine/tailor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/tailor.git"
  },
  "scripts": {
    "prebuild": "del-cli dist",
    "build": "vite build",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:scss": "stylelint \"**/*.vue\" \"**/*.scss\""
  },
  "dependencies": {
    "@extensionengine/vue-radio": "^1.0.0-beta.4",
    "@tailor-cms/utils": "^1.0.0-beta.19",
    "lodash": "^4.17.21",
    "p-min-delay": "^4.0.2",
    "pluralize": "^8.0.0",
    "vue-quill-editor": "^3.0.6",
    "vuedraggable": "^2.24.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@extensionengine/eslint-config": "^4.0.0",
    "@extensionengine/stylelint-config": "^2.0.0",
    "@babel/eslint-plugin": "^7.22.5",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/compiler-sfc": "^2.7.14",
    "del-cli": "^5.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "9.15.1",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "vite": "^4.4.1",
    "vue-template-compiler": "^2.7.14"
  },
  "peerDependencies": {
    "vee-validate": "^3.4.5",
    "vuetify": "^2.6.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
