{
  "name": "tailor",
  "version": "5.0.0-rc.5",
  "codename": "twill",
  "description": "Content authoring platform",
  "author": "ExtensionEngine <info@extensionengine.com>",
  "private": true,
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "postinstall": "if [ $BUILD_ASSETS ]; then npm run build; fi",
    "postshrinkwrap": "rewrite-lockfile package-lock.json",
    "start": "node -r ./server/script/preflight ./server",
    "dev:client": "poi --serve",
    "build": "del-cli node_modules/.cache/vue-loader && poi --prod",
    "packages:bootstrap": "lerna bootstrap",
    "packages:build": "lerna run build --",
    "packages:link": "npm link ./packages/*",
    "packages:setup": "npm run packages:bootstrap && npm run packages:build && npm run packages:link",
    "packages:publish": "lerna publish --",
    "clean": "del-cli dist/* !dist/.gitkeep",
    "webpack:inspect-config": "poi --inspect-webpack",
    "webpack:bundle-report": "poi --prod --bundle-report",
    "webpack:stats": "poi --prod --print-stats",
    "dev:server": "nodemon -r ./server/script/preflight ./server | bunyan-pretty",
    "debug:server": "nodemon --inspect -r ./server/script/preflight ./server",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "ci:e2e": "dotenv -- bash -c 'start-server-and-test start :${PORT} cy:run'",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:scss": "stylelint \"client/**/*.vue\" \"client/**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "db": "node -r ./server/script/preflight ./server/script/sequelize",
    "db:reset": "npm run db drop && npm run db create && npm run db migrate",
    "db:seed": "npm run db seed:all",
    "add:admin": "node -r ./server/script/preflight ./server/script/addAdmin.js",
    "invite:admin": "node -r ./server/script/preflight ./server/script/inviteAdmin.js",
    "integration:add": "node -r ./server/script/preflight ./server/script/addIntegration.js",
    "integration:token": "node -r ./server/script/preflight ./server/script/generateIntegrationToken.js",
    "detachDeletedRepos": "node -r ./server/script/preflight ./server/script/detachDeletedRepos.js",
    "migrateAssessmentPools": "node -r ./server/script/preflight ./server/script/migrateAssessmentPools.js",
    "migrateAssetsLocation": "node -r ./server/script/preflight ./server/script/migrateAssetsLocation.js",
    "fixRevisionUrls": "node -r ./server/script/preflight ./server/script/fixRevisionUrls.js"
  },
