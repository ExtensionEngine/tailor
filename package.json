{
  "name": "tailor",
  "version": "5.0.0-rc.5",
  "codename": "twill",
  "description": "Content authoring platform",
  "author": "ExtensionEngine <info@extensionengine.com>",
  "private": true,
  "scripts": {
    "postinstall": "if [ $BUILD_ASSETS ]; then npm run build; fi",
    "postshrinkwrap": "rewrite-lockfile package-lock.json",
    "start": "node --import ./server/script/preflight.js ./server/index.js",
    "dev:client": "vite serve",
    "build": "del-cli node_modules/.vite && vite build",
    "packages:bootstrap": "lerna bootstrap",
    "packages:build": "lerna run build --",
    "packages:link": "npm link ./packages/*",
    "packages:setup": "npm run packages:bootstrap && npm run packages:build && npm run packages:link",
    "packages:publish": "lerna publish --",
    "clean": "del-cli dist/* !dist/.gitkeep",
    "dev:server": "nodemon --import ./server/script/preflight.js ./server/index.js | bunyan-pretty",
    "debug:server": "nodemon --inspect --import ./server/script/preflight.js ./server/index.js",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "ci:e2e": "dotenv -- bash -c 'start-server-and-test start :${PORT} cy:run'",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:scss": "stylelint \"client/**/*.vue\" \"client/**/*.scss\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "db": "node --import ./server/script/preflight.js ./server/script/sequelize.js",
    "db:reset": "npm run db drop && npm run db create && npm run db migrate",
    "db:seed": "npm run db seed:all",
    "add:admin": "node --import ./server/script/preflight.js ./server/script/addAdmin.js",
    "invite:admin": "node --import ./server/script/preflight.js ./server/script/inviteAdmin.js",
    "integration:add": "node --import ./server/script/preflight.js ./server/script/addIntegration.js",
    "integration:token": "node --import ./server/script/preflight.js ./server/script/generateIntegrationToken.js",
    "detachDeletedRepos": "node --import ./server/script/preflight.js ./server/script/detachDeletedRepos.js",
    "migrateAssessmentPools": "node --import ./server/script/preflight.js ./server/script/migrateAssessmentPools.js",
    "migrateAssetsLocation": "node --import ./server/script/preflight.js ./server/script/migrateAssetsLocation.js",
    "fixRevisionUrls": "node --import ./server/script/preflight.js ./server/script/fixRevisionUrls.js"
  },
